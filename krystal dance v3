        {"CrackDown", "https://raw.githubusercontent.com/Solary-3/Scripts/refs/heads/main/CrackDown.lua"},
        {"Metro", "https://raw.githubusercontent.com/Solary-3/Scripts/refs/heads/main/Metro.lua"}
